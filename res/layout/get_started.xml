<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/background">
    <RelativeLayout
            android:id="@+id/first_partner_rlLoginFaceBook"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            >
        <Button
                android:id="@+id/first_partner_btFindYourPartner"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_find_partner_normal"
                android:layout_centerHorizontal="true"/>
        <TextView
                android:id="@+id/first_partner_tvFindYourPartner"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/normal_style_text"
                android:textColor="@color/White"
                android:textSize="@dimen/_20dp"
                android:layout_below="@+id/first_partner_btFindYourPartner"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/_10dp"
                android:text="Find your partner"/>
    </RelativeLayout>
    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"></RelativeLayout>
</RelativeLayout>